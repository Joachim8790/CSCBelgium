@model CSCBelgium.ViewModels.TestViewModel

@{
    ViewBag.Title = "DitIsEenTestPaginaEnHierKanJeNietsOpDoen";
    Layout = "~/Views/Shared/_LayoutConfiguratiepaneel.cshtml";
}
@using CSCBelgium.DAO.Model;

<h2>DitIsEenTestPaginaEnHierKanJeNietsOpDoen</h2>

@for (int i = 0; i < Model.afbeeldingen.Count(); i++) 
{
